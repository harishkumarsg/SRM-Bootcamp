Day 2
Program 6: House robber
class Solution {
    public int rob(int[] nums) {
        int prevNotRob = 0; // dp[i-2]
        int prevRob = 0;    // dp[i-1]

        for (int money : nums) {
            int current = Math.max(prevRob, prevNotRob + money);
            prevNotRob = prevRob;
            prevRob = current;
        }

        return prevRob;
    }
}
